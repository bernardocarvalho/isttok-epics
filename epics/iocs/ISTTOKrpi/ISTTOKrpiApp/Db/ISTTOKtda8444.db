#####################################################
# Read/Write tda8444 register @ address 0x20=d32 ## #
#####################################################
record( longout, "$(P)$(R)TDA8444:32:DAC_CH0") {
	field(DTYP, "stream")
	field(OUT,  "@tda8444.proto wDac0(32) I2C")
	field(DRVH, "63" )
        field(DRVL, "0" )
	field(VAL, 32)
	field(PINI,"YES")
}
