record(ai, "$(P)$(R)Temperature_0")
{
   field(PINI, "1")
   field(DTYP, "asynFloat64")
   field(INP,  "@asyn($(PORT),$(ADDR),$(TIMEOUT))PIC_TEMP_0_VAL")
# Precision
#   field(PREC, "5")
#  Engineering Units
   field(EGU, "ÂºC")
# Convertion
   field(LINR, "LINEAR")
# val = val * ESLO + EGUL
   field(ESLO, "1/1024")
   field(EGUL, "0.0")
   field(DESC,"Thermocouple 0") 
   field(SCAN, "I/O Intr")
   field(HIHI,"90.0")
   field(HIGH,"50.0")
   field(LOW, "20.0")
   field(LOLO,"0.0")
   field(HHSV,"MAJOR")
   field(HSV, "MINOR")
   field(LSV, "MINOR")
   field(LLSV,"MAJOR")
   field(ADEL, "1.0")      # Archive Deadband
   field(MDEL, "0.0")      # Monitor Deadband
}


